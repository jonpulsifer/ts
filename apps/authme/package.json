{
  "name": "authme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "prisma generate --no-hints && next build",
    "build:certs": "./env2files.sh && prisma generate --no-hints && prisma db push && next build",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:generate": "prisma generate --no-hints",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && prisma migrate dev --name init",
    "db:studio": "prisma studio",
    "start": "next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.7.0",
    "@prisma/client": "5.20.0",
    "@repo/ui": "workspace:*",
    "next-auth": "4.24.8",
    "next": "14.2.15",
    "react-dom": "18.3.1",
    "react": "18.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.0.3",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "20.16.11",
    "@types/react-dom": "18.3.1",
    "@types/react": "18.3.11",
    "prisma": "5.20.0",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
