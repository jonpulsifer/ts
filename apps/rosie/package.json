    "@types/node-fetch": "2.6.11",
    "@repo/eslint-config": "workspace:*",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "@repo/typescript-config": "workspace:*",
    "typescript": "5.3.3"
  }
