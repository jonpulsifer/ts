{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./avatar": "./src/avatar.tsx",
    "./button": "./src/button.tsx",
    "./badge": "./src/badge.tsx",
    "./card": "./src/card.tsx",
    "./table": "./src/table.tsx",
    "./text": "./src/text.tsx"
  },
  "license": "MIT",
  "scripts": {
