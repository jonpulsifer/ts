    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "14.0.4",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "18.2.47",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.33",
    "react": "18.2.0",
    "tailwindcss": "3.4.1",
    "tsup": "8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@headlessui/react": "2.0.0-alpha.4",
    "@heroicons/react": "^2.1.1",
    "clsx": "^2.1.0"
