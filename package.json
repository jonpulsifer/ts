{
  "name": "ts",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "prettier": "3.2.5",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "turbo": "1.12.3"
  },
  "engines": {
    "node": ">=18.*.*",
    "npm": ">=9.*.*"
  },
  "packageManager": "pnpm@8.15.2",
  "cspell": {
    "version": "0.2",
    "ignorePaths": [],
    "dictionaryDefinitions": [],
    "dictionaries": [],
    "words": [
      "nestjs"
    ],
    "ignoreWords": [],
    "import": []
